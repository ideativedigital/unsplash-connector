{
	"name": "ideative/unsplash-connector",
	"type": "typo3-cms-extension",
	"description": "Plugin for extension id_stock_pictures that connects the Unsplash API to the TYPO3 backend",
	"autoload": {
		"psr-4": {
			"Ideative\\IdUnsplashConnector\\": "Classes"
		}
	},
	"authors": [
		{
			"name": "Julien Henchoz",
			"email": "typo3@ideative.ch",
			"role": "Developer",
			"homepage": "https://www.ideative.ch"
		}
	],
	"require": {
		"typo3/cms-core": "^12",
		"ideative/stock-pictures": "^12"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "id_unsplash_connector"
		}
	},
	"homepage": "https://github.com/ideativedigital/unsplash-connector",
	"license": "GPL-3.0+",
	"support": {
		"issues": "https://github.com/ideativedigital/unsplash-connector"
	}
}
